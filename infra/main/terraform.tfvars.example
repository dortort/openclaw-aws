region              = "REPLACE_WITH_REGION"
project_name        = "openclaw"
vpc_cidr            = "10.0.0.0/16"
private_subnet_cidrs = ["10.0.1.0/24", "10.0.2.0/24"]
public_subnet_cidrs  = ["10.0.101.0/24", "10.0.102.0/24"]
enable_nat          = false

app_port         = 8080
health_check_path = "/health"

ecr_repository_name  = "openclaw-gateway"
# Set exactly one of the following:
# gateway_image_tag   = "vX.Y.Z"
gateway_image_digest = "sha256:REPLACE_WITH_DIGEST"
# Provide via TF_VAR_gateway_token or set here.
# gateway_token = "REPLACE_WITH_TOKEN"

# Fargate CPU/memory – must be a valid combination:
#   256 CPU  →  512, 1024, 2048 MiB
#   512 CPU  →  1024–4096 MiB  (1024 step)
#  1024 CPU  →  2048–8192 MiB  (1024 step)
#  2048 CPU  →  4096–16384 MiB (1024 step)
#  4096 CPU  →  8192–30720 MiB (1024 step)
#  8192 CPU  → 16384–61440 MiB (4096 step)
# 16384 CPU  → 32768–122880 MiB (8192 step)
ecs_cpu    = 512
ecs_memory = 1024

efs_posix_uid = 1000
efs_posix_gid = 1000

tailnet_cidrs      = []
tailscale_ssh_cidrs = []
enable_tailscale_router = false
